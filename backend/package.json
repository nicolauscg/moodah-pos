{
  "name": "moodah-pos-backend",
  "version": "1.0.0",
  "private": true,
  "description": "odoo middleware graphql apollo server",
  "main": "handler.js",
  "scripts": {
    "start": "sls offline --port 3001",
    "test": "jest",
    "lint": "npm run lint:all",
    "lint:all": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:graphql": "eslint src/**/*.ts handler.ts",
    "gen-docs": "rm -rf ./doc/schema && graphdoc -e http://localhost:3001/graphql -o ./doc/schema",
    "deploy-docs-netlify": "npm run gen-docs && netlify deploy --prod --dir doc/schema"
  },
  "dependencies": {
    "@types/graphql-relay": "^0.4.11",
    "@types/humps": "^1.1.2",
    "apollo-server-lambda": "^2.3.1",
    "aws-lambda": "^0.1.2",
    "btoa": "^1.2.1",
    "dataloader": "^1.4.0",
    "date-fns": "^1.30.1",
    "fp-ts": "^1.9.0",
    "graphql": "^14.0.2",
    "graphql-fields-list": "^2.1.1",
    "graphql-playground-middleware-lambda": "^1.7.6",
    "graphql-relay": "^0.6.0",
    "graphql-resolvers": "^0.3.2",
    "humps": "^2.0.1",
    "nodoo": "^7.2.0",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.12.18",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "apollo-server-testing": "^2.9.3",
    "babel": "^6.23.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "serverless-offline": "~4.9.4",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.5.3",
    "webpack": "^4.29.0",
    "xstream": "^11.10.0"
  },
  "author": "moodah-pos",
  "license": "MIT"
}
